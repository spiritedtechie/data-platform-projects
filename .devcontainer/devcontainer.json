{
  "name": "data-platform-projects",
  "build": {
    "dockerfile": "Dockerfile",
    "args": {
      "USER_UID": "501",
      "USER_GID": "20"
    }
  },
  "postCreateCommand": "sudo chown $(id -u):$(id -g) /workspaces/data-platform-projects",
  "remoteUser": "vscode",
  "containerUser": "vscode",
  "customizations": {
    "vscode": {
      "extensions": [
        "openai.chatgpt",
        "dbtlabsinc.dbt",
        "henriblancke.vscode-dbt-formatter",
        "ms-python.python",
        "ms-python.debugpy"
      ]
    }
  }
}